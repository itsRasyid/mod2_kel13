<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.modul2_kel13">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Modul2_kel07">
        <activity
            android:name="com.example.modul2_kel13.AboutActivity"
            android:parentActivityName="com.example.modul2_kel13.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.example.modul2_kel13.DetailActivity"
            android:parentActivityName="com.example.modul2_kel13.MainActivity" />
        <activity
            android:name="com.example.modul2_kel13.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.modul2_kel13.LandingPageActivity" />
        <activity
            android:name="com.example.modul2_kel13.MainActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>